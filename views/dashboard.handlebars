<section>
    <h1>Welcome to the Dashboard</h1>
</section>

{{#if posts.length }}
<section>
    <h2>Your Posts</h2>
    <ol>
        {{#each posts as |post|}}
        <li>
            {{> post-info post}}
        </li>
        {{/each}}
    </ol>
</section>
{{/if}}
 <button id="add-post">Add Post +</button>
